unit uGrupos;

interface
uses
  Classes, SysUtils, uPai, uCidades, Vcl.ComCtrls;

type
   Grupos = class (Pai)

  protected
   Grupo  	      :string[100];
   Situacao	      :string[1];
   Observacao	    :string[100];



  public
   constructor CrieObj;
   destructor Destrua_se;
